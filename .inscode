run = "pip install -r requirements.txt && streamlit run app.py --server.port=8501 --server.address=0.0.0.0"
language = "python"
is_html = false
is_resident = false
is_gui = false

[packager]
AUTO_PIP = true

[env]
PATH = "${VENV_PATH}\\Scripts;${PATH}"
PYTHONPATH = "${VENV_PATH}\\Lib\\site-packages"
MPLBACKEND = "Agg"

[debugger]
program = "app.py"